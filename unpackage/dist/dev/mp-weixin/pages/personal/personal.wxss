page {
  background-color: #FFFFFF;
}
.container {
  margin: 40rpx 40rpx;
}
.container .user_info {
  background-image: url(data:image/png;base64,);
  height: 300rpx;
  border-radius: 16rpx;
  background-color: #FFFFFF;
  box-shadow: 0 1rpx 16rpx rgba(0, 0, 0, 0.1);
}
.container .user_info .info_avatarUrl {
  width: 150rpx;
  height: 150rpx;
  border-radius: 20rpx;
  margin-bottom: 30rpx;
}
.container .user_info .info_name {
  font-size: 40rpx;
}
.container .user_content {
  margin-top: 80rpx;
  height: 800rpx;
}
.container .user_content .content_img {
  margin-right: 20rpx;
  width: 40rpx;
  height: 40rpx;
}
.container .user_content .user_item {
  margin: 0 20rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
}
.container .user_content .user_item .cuIcon-right {
  float: right;
}
.container .user_content .user_item:first-child {
  border-top: 0.5px solid #787878;
}
.container .loing_btn {
  margin-top: 40rpx;
}
.container .loing_btn .cu-btn {
  /* border-radius: 25px; */
  width: 600rpx;
  height: 100rpx;
  color: #fff;
  background-color: #59b58d;
}

